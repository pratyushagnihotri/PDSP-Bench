{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport logo from '@/assets/app_logo.png';\nimport bg from \"@/assets/bg.jpg\";\nimport axios from 'axios';\nexport default {\n  name: 'LandingPage',\n  components: {},\n  data() {\n    return {\n      url: process.env.VUE_APP_URL,\n      dsplogo: logo,\n      bgImg: bg,\n      username: '',\n      password: '',\n      nameRules: [v => !!v || 'Name is required', v => v.length <= 10 || 'Name must be less than 10 characters'],\n      rules: {\n        required: value => !!value || 'Required.',\n        min: v => v.length >= 8 || 'Min 8 characters'\n      },\n      problem: false,\n      problemmsg: '',\n      alerttype: ''\n    };\n  },\n  methods: {\n    async loginUser() {\n      this.problem = false;\n      /* Aggregating the form elements into 'data' variable */\n      var data = {\n        username: this.username,\n        password: this.password\n      };\n      /* Send Request to backend */\n      await axios.post(this.url + ':8000/auth/login', data).then(async response => {\n        var resp = await response.data;\n        if (resp.success) {\n          this.$router.push('dashboard/createCluster');\n        }\n      }).catch(async error => {\n        var err = await error.response.data;\n        this.problem = !this.problem;\n        this.problemmsg = err.status;\n        this.alerttype = 'error';\n        this.snackbar = {\n          view: true,\n          timeout: 4000,\n          text: 'Invalid username/password',\n          color: 'error'\n        };\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";AA0EA;AACA;AACA;AACA;EACAA;EACAC;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC,YACAC,gCACAA,8DACA;MAEAC;QACAC;QACAC;MACA;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;MACA;MACA;MACA;QACAV;QACAC;MACA;MACA;MACA,YACAU,0CACAC;QACA;QACA;UACA;QACA;MACA,GACAC;QACA;QACA;QACA;QACA;QACA;UACAC;UACAC;UACAC;UACAC;QACA;MACA;IACA;EACA;AACA","names":["name","components","data","url","dsplogo","bgImg","usernam